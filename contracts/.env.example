# NeoX Deployment
NEOX_PRIVATE_KEY=0x...  # Deployer wallet (needs GAS)
NEOX_RPC_URL=https://testnet.rpc.banelabs.org

# Wallet Addresses for Minting USDC
BUTLER_ADDRESS=0x...    # Butler (User) wallet address
WORKER_ADDRESS=0x...    # Worker agent wallet address
MANAGER_ADDRESS=0x...   # Manager agent wallet address
SCRAPER_ADDRESS=0x...   # Scraper agent wallet address
CALLER_ADDRESS=0x...    # Caller agent wallet address

# Minting Settings
MINT_AMOUNT=10000  # USDC per address (default: 10,000)
