# NeoX Network Configuration
NEOX_RPC_URL=https://testnet.rpc.banelabs.org
NEOX_CHAIN_ID=12227332

# WebSocket for event listening (optional, falls back to polling)
NEOX_WS_URL=wss://testnet.rpc.banelabs.org

# =============================================================================
# AGENT WALLETS
# Each agent has its own wallet for transactions and signing
# Generate new wallets with: python -c "from eth_account import Account; a = Account.create(); print(f'Address: {a.address}\nPrivate Key: {a.key.hex()}')"
# =============================================================================
MANAGER_PRIVATE_KEY=
SCRAPER_PRIVATE_KEY=
CALLER_PRIVATE_KEY=

# =============================================================================
# LLM PROVIDER
# Required for agent decision-making (use Anthropic or OpenAI)
# =============================================================================
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# =============================================================================
# EXTERNAL APIS
# =============================================================================

# Bright Data (for TikTok and web scraping)
# Get from: https://brightdata.com/
BRIGHTDATA_API_TOKEN=
BRIGHTDATA_ZONE=

# Twilio (for phone calls)
# Get from: https://console.twilio.com/
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=+1XXXXXXXXXX

# =============================================================================
# NEOFS STORAGE
# Decentralized storage for job results and proof-of-work
# =============================================================================
NEOFS_REST_GATEWAY=http://rest.t5.fs.neo.org:8080
NEOFS_CONTAINER_ID=

# =============================================================================
# AGENT A2A ENDPOINTS
# For local development or production deployment
# =============================================================================
MANAGER_ENDPOINT=http://localhost:3001
SCRAPER_ENDPOINT=http://localhost:3002
CALLER_ENDPOINT=http://localhost:3003

# Agent server ports
MANAGER_PORT=3001
SCRAPER_PORT=3002
CALLER_PORT=3003

# =============================================================================
# SMART CONTRACT ADDRESSES
# Filled after deployment to NeoX (see contracts/deployments/)
# =============================================================================
ORDERBOOK_ADDRESS=
ESCROW_ADDRESS=
JOB_REGISTRY_ADDRESS=
AGENT_REGISTRY_ADDRESS=
REPUTATION_TOKEN_ADDRESS=
USDC_ADDRESS=

# =============================================================================
# BIDDING CONFIGURATION (optional)
# =============================================================================
# Minimum profit margin for bidding (0.0 to 1.0)
MIN_PROFIT_MARGIN=0.10
# Maximum concurrent jobs per agent
MAX_CONCURRENT_JOBS=5
# Auto-bid when job matches capabilities (true/false)
AUTO_BID_ENABLED=true
